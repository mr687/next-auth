{
  "name": "@auth/typegoose-adapter",
  "version": "1.0.0",
  "description": "Typegoose adapter for Auth.js.",
  "homepage": "https://authjs.dev/reference/adapter/typegoose",
  "repository": "https://github.com/nextauthjs/next-auth",
  "bugs": {
    "url": "https://github.com/nextauthjs/next-auth/issues"
  },
  "author": "Davi Nomoeh Dani <dapino.dev@gmail.com>",
  "contributors": [
    "Balázs Orbán <info@balazsorban.com>"
  ],
  "type": "module",
  "types": "./index.d.ts",
  "license": "ISC",
  "files": [
    "*.js",
    "*.d.ts*",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "keywords": [
    "next-auth",
    "next.js",
    "oauth",
    "typegoose",
    "adapter"
  ],
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "./tests/test.sh",
    "test:watch": "./tests/test.sh -w",
    "build": "tsc"
  },
  "dependencies": {
    "class-transformer": "^0.5.1"
  },
  "peerDependencies": {
    "next-auth": "^4",
    "@typegoose/typegoose": "^11.2.0",
    "mongoose": "^7.2.2"
  },
  "devDependencies": {
    "@next-auth/adapter-test": "workspace:*",
    "@next-auth/tsconfig": "workspace:*",
    "jest": "^27.4.3",
    "next-auth": "workspace:*",
    "@typegoose/typegoose": "^11.2.0",
    "mongoose": "^7.2.2"
  },
  "jest": {
    "preset": "@next-auth/adapter-test/jest"
  }
}
